<Window x:Class="ProyectoDiagnos.UI.Modeler"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoDiagnos.UI"
        mc:Ignorable="d"
        Title="Modeler" Height="450" Width="950">
    <Canvas Margin="4,0,538,0" Height="30" VerticalAlignment="Top">
        <Rectangle Fill="#009EC2CD" Height="30" Stroke="#FF0078D7" Width="400" Canvas.Top="-1"/>
        <Label Content="{Binding rut}" Foreground="#FF0078D7" FontSize="16" Width="142" HorizontalContentAlignment="Center" Height="28" Canvas.Top="-1"/>
        <Label Content="{Binding NombreCompleto}" Canvas.Left="150" Foreground="#FF0078D7" FontSize="14" Canvas.Top="-1"/>
        <Rectangle Fill="#FF0078D7" Height="20" Stroke="#01000000" VerticalAlignment="Top" Width="3" Canvas.Left="142" Canvas.Top="3"/>
    </Canvas>
</Window>
